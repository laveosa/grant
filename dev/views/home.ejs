<!DOCTYPE html>
<html lang="en">
  <%- include('./layouts/head.ejs')%>
  <body>
    <div class="main-wraper">
      <%- include('./layouts/navigation.ejs')%>
      <div class="page-content-cover">
        <section class="container">
          <h1 class="text-center m-3">Home page</h1>
          <div class="content-navigation-tabs-container">
            <div class="navigation-tabs-area">
              <div class="nav">
                <div class="nav-item">
                  <a href="/home/dashboard" class="nav-link">dashboard</a>
                </div>
                <div class="nav-item">
                  <a href="/home/messages" class="nav-link">messages</a>
                </div>
                <div class="nav-item">
                  <a href="/home/stats" class="nav-link">stats</a>
                </div>
              </div>
            </div>
            <div class="content-area">
              <% if(content.contentUrl){ %> <%- include(content.contentUrl)%>
              <%} else {%>
              <div class="no-content-selected">
                <span>no content selected</span>
              </div>
              <%}%>
            </div>
          </div>
        </section>
      </div>
    </div>
  </body>
</html>
